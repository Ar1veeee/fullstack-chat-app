{
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { 
      "src": "/api/(.*)", 
      "dest": "dist/server.js" 
    },
    { 
      "src": "/(.*)", 
      "dest": "dist/server.js" 
    }
  ]
}