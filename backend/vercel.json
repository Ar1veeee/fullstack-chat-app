{
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "startCommand": "npm run prod",
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}